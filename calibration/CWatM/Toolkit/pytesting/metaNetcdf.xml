<metanetcdf varname="FrostIndex" unit="degree/days"  standard_name="FrostIndex" long_name="Frost index based on Molnau, Bissel (1983)"/>

# Groundwater
<metanetcdf varname="sum_per3toGW" unit="m"  standard_name="Percolation_w3_to_groundwater" long_name="Percolation from 3rd soil layer to groundwater"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_prefFlow" unit="m"  standard_name="Preferential_flow_through_soil" long_name="Preferential flow to groundwater"  title="CWATM" author="IIASA WAT" />
