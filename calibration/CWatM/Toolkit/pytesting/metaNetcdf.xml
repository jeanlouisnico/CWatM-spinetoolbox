<metanetcdf varname="totalSto" unit="m"  standard_name="Total_storage_soil" long_name="Total storage of soil"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_topwater" unit="m"  standard_name="Total_storage_topwater" long_name="Total storage of water on top of soil"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_w1" unit="m"  standard_name="Total_storage_w1" long_name="Total storage of water 1st soil layer"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_w2" unit="m"  standard_name="Total_storage_w2" long_name="Total storage of water 2nd soil layer"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_w3" unit="m"  standard_name="Total_storage_w3" long_name="Total storage of water 3rd soil layer"  title="CWATM" author="IIASA WAT" />

# Environmental Flow
<metanetcdf varname="envFlow" unit="m"  standard_name="Environmental_flow" long_name="Environmental flow"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="envFlowm3s" unit="m3/s"  standard_name="Environmental_flow" long_name="Environmental flow in m3/s"  title="CWATM" author="IIASA WAT" />


