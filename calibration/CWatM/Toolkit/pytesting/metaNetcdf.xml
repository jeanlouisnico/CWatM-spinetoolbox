<metanetcdf varname="storGroundwater" unit="m"  standard_name="Storage_groundwater" long_name="Storage groundwater"  title="CWATM" author="IIASA WAT" />


# Runoff
<metanetcdf varname="runoff" unit="m"  standard_name="Runoff" long_name="Total runoff from surface, interflow and groundwater"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_landSurfaceRunoff " unit="m"  standard_name="Landsurface_runoff" long_name="Runoff from surface and interflow"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_directRunoff" unit="m"  standard_name="Direct_runoff" long_name="Runoff from surface"  title="CWATM" author="IIASA WAT" />
<metanetcdf varname="sum_interflow" unit="m"  standard_name="Interflow_runoff" long_name="Runoff from interflow"  title="CWATM" author="IIASA WAT" />

